use slex.*

class Int:
    i32 value
    
    def Int(i32 number):
        self.value = number

    @operator
    def +(i32 value): Int*
        return new Int(self.value + value)

def main(): i32
    Int* a = new Int(10)
    Int* b = new Int(20)
    
    Int* c = a + b
    
    del a
    del b
    del c
    return 0
