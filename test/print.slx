use slex.*

class Int:
    i32 value
    
    def Int():
    
    def Int(i32 number):
        self.value = number

    @operator
    def +(Int b): i32
        return self.value + b.value
        

def main(): i32
    Int a, b, c
    a.value = 10
    b.value = 20
    
    c.value = a + b
    
    print(c.value)
    
    return 0
